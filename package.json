{
  "name": "cypress-mutation-example",
  "version": "1.0.0",
  "description": "Mutation testing example using Cypress",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "react": "16",
    "react-dom": "16",
    "react-scripts": "4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:coverage": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "start-test 3000 cy:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:mutations": "stryker run",
    "ci": "start-test 3000 cy:run"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/first": "warn"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/cypress-mutation-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bahmutov/cypress-mutation-example/issues"
  },
  "homepage": "https://github.com/bahmutov/cypress-mutation-example#readme",
  "devDependencies": {
    "@cypress/code-coverage": "3.8.8",
    "@cypress/instrument-cra": "1.4.0",
    "@stryker-mutator/core": "4.3.1",
    "cypress": "6.2.0",
    "prettier": "2.2.1",
    "start-server-and-test": "1.11.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
