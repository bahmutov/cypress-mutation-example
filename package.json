{
  "name": "cypress-mutation-example",
  "version": "1.0.0",
  "description": "Mutation testing example using Cypress",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "react": "16",
    "react-dom": "16",
    "react-scripts": "3.2"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
